/*--------------------------------Check box--------------------------------*/
QCheckBox {
	color:#999999;
	font: italic 9pt &quot;Open Sans Light&quot;;
	border-radius:6px;
}

QCheckBox::indicator:checked {
    background: #198C49;
	border-radius:6px;
}

QCheckBox::indicator:unchecked {
    background:#990329;
	border-radius:6px;
}

QCheckBox::indicator:checked:hover {
  	border: 2px solid #198C49;
    background: #15733C;
	border-radius:6px;
}

